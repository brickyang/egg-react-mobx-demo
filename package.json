{
  "name": "react-mobx-demo",
  "version": "0.0.1",
  "description": "A demo of using egg.js, react and mobx.",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "egg-view-nunjucks": "^2.1.3",
    "lodash.uniqueid": "^4.0.1",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "mobx-react-devtools": "^4.2.15",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "egg-bin": "^4.1.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.9.0",
    "eslint": "^4.4.0",
    "eslint-config-egg": "^6.0.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "prebuild": "rm -rf build/*",
    "build": "webpack --config config/webpack.config.js -p --progress --colors",
    "dev": "npm run dev-webpack & egg-bin dev",
    "dev-webpack": "node webpack-dev-server",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brickyang/egg-react-mobx-demo.git"
  },
