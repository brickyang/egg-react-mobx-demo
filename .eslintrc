{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    // "react/jsx-no-undef": 2,
    // "react/react-in-jsx-scope": 2
  },
  "plugins": ["react"],
  "extends": "eslint-config-egg"
}
