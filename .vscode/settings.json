{
  "files.exclude": {
    "USE_GITIGNORE": true,
    "**/*.js": { "when": "$(basename).ts" },
    "**/**.js": { "when": "$(basename).tsx" },
    "**/*.map": true,
    "run": true,
    "logs": true,
    "out": true,
    "node_modules": true,
    "coverage": true
  },
  "javascript.implicitProjectConfig.experimentalDecorators": true
}
